<h1>Listing users</h1>
	<% if notice %>
	<p id="notice"><%= notice %></p>
	<% end %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  	<tr>
    	<td> <%= User.current_user.name %></td>
    	<td> <%= User.current_user.email %></td>
    </tr>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
<%= link_to 'Log Out', sessions_destroy_url %>
